{
  "name": "customer-app-backend",
  "version": "1.0.0",
  "description": "Backend for Customer React App",
  "main": "server.js",
  "engines": {
    "node": ">=20.18.0 <21",
    "npm": ">=9.9.0 <10"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-db": "node scripts/setup-user-tracking.js",
    "setup-db-only": "node scripts/setup-user-tracking.js",
    "test-db": "node -e \"require('./db').poolPromise.then(() => console.log('✅ Database connection successful')).catch(err => console.error('❌ Database connection failed:', err))\"",
    "diagnose-sql": "node scripts/diagnose-azure-sql.js",
    "test-azure-ad": "node scripts/test-azure-ad-connection.js",
    "test-org-access": "node scripts/test-organizational-access.js",
    "verify-setup": "node scripts/verify-organizational-setup.js",
    "complete-setup": "node scripts/complete-setup.js",
    "simple-setup": "node scripts/simple-db-setup.js",
    "create-procedures": "node scripts/create-stored-procedures.js",
    "fix-procedure": "node scripts/fix-stored-procedure.js",
    "fix-connection-old": "node scripts/diagnose-azure-sql.js",
    "reset-db": "echo 'WARNING: This will drop and recreate user tracking tables' && node scripts/reset-user-tracking.js",
    "fix-connection": "node scripts/test-azure-ad-connection.js",
    "check-setup": "node scripts/check-azure-setup.js",
    "test-db-operations": "node scripts/test-database-operations.js",
    "test-app-db": "node scripts/test-application-db.js",
    "diagnose-firewall": "node scripts/diagnose-firewall-issue.js",
    "setup-production-firewall": "node scripts/setup-production-firewall.js",
    "add-my-ip": "node scripts/manage-ip.js add",
    "remove-my-ip": "node scripts/manage-ip.js remove",
    "list-firewall-rules": "node scripts/manage-ip.js list",
    "ip-status": "node scripts/manage-ip.js status",
    "test-with-ip": "npm run add-my-ip && echo 'Waiting 10 seconds...' && sleep 10 && npm run test-azure-ad"
  },
  "dependencies": {
    "@azure/identity": "^4.10.2",
    "@azure/msal-browser": "^3.7.1",
    "@azure/msal-react": "^2.0.8",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.19.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dns": "^0.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
